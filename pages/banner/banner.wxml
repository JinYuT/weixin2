<view class='top'>
  <input placeholder='搜索'>
    <image src=''></image>
  </input>

  <swiper indicator-dots="{{indicatorDots}}"
    autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}">
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image"/>
      </swiper-item>
    </block>
  </swiper>
  
  <view class='banner'>
      <view class='banner_1'>养老</view>
      <view class='banner_2' bindtap='an'>医疗</view>
      <view class='banner_3'>购物</view>
  </view>
</view>

<scroll-view class='main'>
  <view class='nav'>
    <view class='nav_left'>123</view>
     <view class='nav_right'>123</view> 
  </view>


  <view class='for'>

    <view wx:for="{{index}}" class='ch'>
        <view>123</view>
        <view class='sh'>{{item.name}}</view> 
    </view>
  </view>
    

</scroll-view>

